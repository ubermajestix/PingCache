<table>
  <tr>
    <th>mac</th>
    <th>location ip</th>
    <th> local name </th>
    <th> local ip </th>
    <th> created_at </th>
  </tr>
<%for dl in @device_locations %>
  <tr>
    <td><%= dl.device.mac %></td>
    <td><%= dl.location.ip %></td>
    <td><%= dl.local_name %></td>
    <td><%= dl.local_ip %></td>
    <td><%= dl.created_at %></td>
  </tr>
<%end%>
</table>